#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vighnesh/Vigh/Coding/Projects/ThreadsClone/push_to_Github/Social_Media_WepApp/backend/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/home/vighnesh/Vigh/Coding/Projects/ThreadsClone/push_to_Github/Social_Media_WepApp/backend/node_modules/.pnpm/rimraf@3.0.2/node_modules:/home/vighnesh/Vigh/Coding/Projects/ThreadsClone/push_to_Github/Social_Media_WepApp/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vighnesh/Vigh/Coding/Projects/ThreadsClone/push_to_Github/Social_Media_WepApp/backend/node_modules/.pnpm/rimraf@3.0.2/node_modules/rimraf/node_modules:/home/vighnesh/Vigh/Coding/Projects/ThreadsClone/push_to_Github/Social_Media_WepApp/backend/node_modules/.pnpm/rimraf@3.0.2/node_modules:/home/vighnesh/Vigh/Coding/Projects/ThreadsClone/push_to_Github/Social_Media_WepApp/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
else
  exec node  "$basedir/../rimraf/bin.js" "$@"
fi
